{
    "name": "ggggino/wordbundle",
    "description": "This is a Symfony2 Bundle helps you to read and write Word files, thanks to the PHPWord library",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "David Ginanni",
            "email": "david.ginanni@gmail.com",
            "homepage": "https://github.com/GGGGino"
        }
    ],
    "keywords": ["Office", "Word", "symfony2", "bundle"],
    "homepage": "https://github.com/GGGGino/WordBundle",
    "license": "MIT",
    "require": {
        "php": ">7.0",
        "symfony/framework-bundle": "^4.0|^5.0",
        "phpoffice/phpword": "^0.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/phpunit-bridge": "^4.4 || ^5.1",
        "symfony/yaml": "^5.2",
        "symfony/browser-kit": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "GGGGino\\WordBundle\\": ""
        },
        "exclude-from-classmap": ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "GGGGino\\WordBundle\\Tests\\": "Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
